<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="org.openmrs.module.pregnancycdss">
  <class name="DiseasesModel" table="form_pregnancycdss_diseases">
    <id name="diseasesId" type="java.lang.Integer" column="diseases_id" unsaved-value="3">
      <generator class="native"/>
    </id>
    <discriminator column="diseases_id" insert="false" />
    
    <property name="diseasesName" type="java.lang.String" column="dis_name" length="100" unique="false"/>
    <property name="diseasesNotes" type="java.lang.String" column="dis_notes" length="255" unique="false"/>
    <property name="diseasesICD10" type="java.lang.String" column="dis_icd10" length="255" unique="false"/>
    <property name="pValue" type="java.lang.Double" column="p_value" unique="false"/>
    <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
  </class>
</hibernate-mapping>