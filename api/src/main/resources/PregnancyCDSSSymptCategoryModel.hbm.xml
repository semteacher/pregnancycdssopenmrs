<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.pregnancycdss">    
    <class name="PregnancyCDSSSymptCategoryModel"
       table="${project.parent.artifactId}_form_pregnancycdss_sympt_category">
        <id name="id" type="int" column="id" unsaved-value="8">
            <generator class="native" />
        </id>
        <discriminator column="id" insert="false" />
        <property name="cat_name" type="java.lang.String" column="cat_name" length="100" unique="false" />
        <property name="cat_notes" type="java.lang.String" column="cat_notes" length="150" unique="false" />
        <property name="is_selected" type="org.hibernate.type.NumericBooleanType" column="is_selected" unique="false" />
    </class>
</hibernate-mapping>
