<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.pregnancycdss">
    <class name="SymptCategoryModel" table="form_pregnancycdss_sympt_category">
        <id name="symptCatId" type="java.lang.Integer" column="sympt_cat_id" unsaved-value="8">
            <generator class="native"/>
        </id>
        <discriminator column="sympt_cat_id" insert="false" />
    
        <property name="catName" type="java.lang.String" column="cat_name" length="100" unique="false"/>
        <property name="catNotes" type="java.lang.String" column="cat_notes" length="150" unique="false"/>
        <property name="isSelected" type="org.hibernate.type.NumericBooleanType" column="is_selected" unique="false"/>
        <list name="symptoms" table="form_pregnancycdss_symptoms">
            <key>
                <column name="id_category" not-null="true"></column>
            </key>
            <list-index column="id_order" />
            <one-to-many class="SymptomModel"/>
        </list>
    </class>
</hibernate-mapping>
