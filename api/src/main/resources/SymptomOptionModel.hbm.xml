<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.pregnancycdss">
    <class name="SymptomOptionModel" table="form_pregnancycdss_sympt_options">
        <id name="symptOptId" type="java.lang.Integer" column="sympt_opt_id" unsaved-value="238">
            <generator class="native"/>
        </id>
        <discriminator column="sympt_opt_id" insert="false" />
    
        <many-to-one name="symptom" class="SymptomModel" column="id_symptom" not-null="true" />
        
        <property name="optName" type="java.lang.String" column="opt_name" length="50" unique="false"/>
        <property name="idOrder" type="java.lang.Integer" column="id_order" unique="false"/>
        <property name="isSelected" type="org.hibernate.type.NumericBooleanType" column="is_selected" unique="false"/>
    </class>
</hibernate-mapping>
