<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                  http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet id="pregnancycdssmodule-2015-11-01" author="SemenetsA">
        <createTable tableName="form_pregnancycdss_diseases">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="dis_name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="dis_note" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="dos_icd10" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>